<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

    <!-- App Display -->
    <key>CFBundleDisplayName</key>
    <string>Driving Theory Test</string>

    <!-- Versioning -->
    <key>CFBundleShortVersionString</key>
    <string>1.0</string>
    <key>CFBundleVersion</key>
    <string>1</string>

    <!-- Launch Screen -->
    <key>UILaunchStoryboardName</key>
    <string>LaunchScreen</string>

    <!-- Notifications (only needed if you actually request notification permission) -->
    <key>NSUserNotificationUsageDescription</key>
    <string>This app can send daily reminders to help you practice.</string>

    <!-- Privacy / Tracking -->
    <key>NSUserTrackingUsageDescription</key>
    <string>We do not track you.</string>

</dict>
</plist>
